{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fanthus.bolt-expo-nativewind",
      "infoPlist": {
        "NSHealthShareUsageDescription": "We need access to your sleep and activity data to provide personalized sleep insights and help you maintain a healthy sleep schedule.",
        "NSHealthUpdateUsageDescription": "We need to update your health data to track your sleep patterns.",
        "UIRequiredDeviceCapabilities": ["healthkit"]
      },
      "entitlements": {
        "com.apple.developer.healthkit": true,
        "com.apple.developer.healthkit.access": []
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.fanthus.boltexponativewind"
    }
  }
}
